@model IEnumerable<AspNetCoreMVC_Food.Data.Models.Category>

<div class="collapse navbar-collapse" id="bs-megadropdown-tabs">
    <ul class="nav navbar-nav nav_1">
        @foreach (var item in Model)
        {
            <li><a asp-controller="Default" asp-action="CategoryDetails" asp-route-id="@item.CategoryId">@item.Name</a></li>
        }
    </ul>
</div>

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>

    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" asp-controller="Default" asp-action="Index">LEZZET-İ TATLAR</a>
            </div>
            <ul class="nav navbar-nav">
                <li><a asp-controller="Category" asp-action="Index">Categories</a></li>
                <li><a asp-controller="Food" asp-action="Index">Foods</a></li>
                <li><a asp-controller="Chart" asp-action="Index">Google Pie Chart</a></li>
                <li><a asp-controller="Chart" asp-action="Index2">Google Column Chart</a></li>
                <li><a asp-controller="Chart" asp-action="Statistic">Statistics</a></li>
                <li><a asp-controller="Login" asp-action="LogOut">Log Out</a></li>
            </ul>
        </div>
    </nav>

    <div class="container">
        @RenderBody()
    </div>

</body>
</html>
